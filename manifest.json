{
  "manifest_version": 3,
  "name": "Custom New Tab",
  "version": "1.0",
  "description": "Overrides the new tab page in Microsoft Edge.",
  "chrome_url_overrides": {
    "newtab": "Browser.html"
  },
  "permissions": ["storage"],
  "action": {
    "default_title": "Custom New Tab"
  }
}
